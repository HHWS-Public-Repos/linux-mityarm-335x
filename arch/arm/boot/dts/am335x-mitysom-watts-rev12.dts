/* RS485 MODBUS 1 */
&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart1_pins>;
	rts-gpio = <&gpio0 4 GPIO_ACTIVE_HIGH>;
	rs485-rts-delay =<0 0>;
	rs485-rts-active-high;
	/*rs485-rx-during-tx;*/
	linux,rs485-enabled-at-boot-time;
	status = "okay";
};

/* RS485 MODBUS 2 */
&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart5_pins>;
	rts-gpio = <&gpio0 28 GPIO_ACTIVE_HIGH>;
	rs485-rts-delay =<0 0>;
	rs485-rts-active-high;
	/*rs485-rx-during-tx;*/
	linux,rs485-enabled-at-boot-time;
	status = "okay";
};


&i2c0 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c0_pins>;

	status = "okay";
	clock-frequency = <400000>;

	/* PCAL9535A 16-bit GPIO expander 0x27 */
	pcal9535: pcal9535@27 {
		compatible = "nxp,pcal9535";
		reg = <0x27>;
		gpio-controller;
		#gpio-cells = <2>;
		gpio-line-names =
		"RELAY_0", "RELAY_1", "RELAY_2", "RELAY_3",
		"", "", "", "",
		"TEMP_IN", "SPARE_IN", "DIGITAL_IN_4", "DIGITAL_IN_3",
		"DIGITAL_IN_2", "DIGITAL_IN_1", "", "";
	};

	eeprom@50 {
		compatible = "st,m24256", "st,24c256", "atmel,24c256";
		reg = <0x50>;
		pagesize = <64>;
	};

	eeprom@51 {
		compatible = "st,m24256", "st,24c256", "atmel,24c256";
		reg = <0x51>;
		pagesize = <64>;
	};

	/* MCP4728 12-bit Quad DAC 0x60 */
};

&i2c1 {
	/* SOM PMIC SmartReflex 0x12 */

	/* GT911 TSC 0x14 */
	gt911@14 {
		compatible = "goodix,gt911";
		reg = <0x14>;
		interrupt-parent = <&gpio0>;
		interrupts = <29 IRQ_TYPE_EDGE_FALLING>;

		irq-gpios = <&gpio0 29 GPIO_ACTIVE_HIGH>;
		// Workaround: Using DISP pin to reset touchscreen
		reset-gpios = <&gpio2 21 GPIO_ACTIVE_HIGH>;
		// touchscreen-swapped-x-y;
	};

	/* SOM Factory EEPROM 0x50 - 0x57 */

	/* MCP7940N RTC 0x6F */
	mcp79400: mcp79400@6f {
		compatible = "microchip,mcp7940x";
		reg = <0x6f>;
	};
};

&usb0 {
	status = "okay";
	dr_mode = "host";
};

&usb1 {
	status = "okay";
	dr_mode = "host";
};

&lcdc {
	status = "okay";
};

&epwmss0 {
	status = "okay";

	ehrpwm0: pwm@48300200 {
		status = "okay";
		pinctrl-names = "ehrpwm0_pins";
		pinctrl-0 = <&ehrpwm0_pins>;
	};
	ecap0: ecap@48300100 {
		compatible = "ti,am62-ecap-capture";
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&ecap_pins>;
	};
};

&epwmss1 {
	status = "okay";
	
};

/ {
	v3_3_analog_fixed: fixedregulator@2 {
		compatible = "regulator-fixed";
		regulator-name = "v3.3_analog_fixed";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
	};
};

&spi1 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi1_pins>;
	/* Not supported: CS0 On-SOM NOR */

	/* Note: spi1 crashes when using more than 4 channels */
	cs-gpios = <&gpio1 15 GPIO_ACTIVE_HIGH>, <&gpio0 5 GPIO_ACTIVE_HIGH>, <&gpio0 18 GPIO_ACTIVE_HIGH>, <&gpio0 20 GPIO_ACTIVE_HIGH>;

	/* SPI1_CS0 - gpio1_15 MCP3208 */
	mcp3208_ain@0 {
		compatible = "microchip,mcp3208";
		reg = <0>;
		spi-max-frequency = <1000000>;
		vref-supply = <&v3_3_analog_fixed>;

		/* channels = "AIN1", "AIN2", "AIN3", "AIN4"; */
	};

	/* SPI1_CS1 - gpio0_5 MCP3208 */
	mcp3208_rtd@1 {
		compatible = "microchip,mcp3208";
		reg = <1>;
		spi-max-frequency = <1000000>;
		vref-supply = <&v3_3_analog_fixed>;

		/* channels = "RTD_IN_HIGH", "RTD_IN_LOW", "AMBIENT"; */
	};

	/* EXT_CS - gpio0_18 J9 Connector */
	ext_j9: ext_j9@2 {
		compatible = "spidev";
		reg = <2>;
		status = "okay";

		spi-max-frequency = <2000000>;
	};

	/* 7SEG_CS - gpio0_20 */
	tlc59281: tlc59281@3 {
		compatible = "spidev";
		reg = <3>;
		status = "okay";

		spi-max-frequency = <2000000>;
	};
};

&mac {
	pinctrl-names = "default";
	pinctrl-0 = <&cpsw_default>;
	status = "okay";
};

&davinci_mdio {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&davinci_mdio_default>;
	pinctrl-1 = <&davinci_mdio_sleep>;
	status = "okay";

	reset-gpios = <&gpio1 28 GPIO_ACTIVE_LOW>,
		      <&gpio3 20 GPIO_ACTIVE_LOW>;
	reset-delay-us = <2>;

	phy0: ethernet-phy@0 {
		reg = <2>;
		/* Restrict phy to 10/100 due to magnetics */
		max-speed = <100>;
	};
};

&cpsw_emac1 {
	phy-handle = <&phy0>;
	phy-mode = "rgmii";
	dual_emac_res_vlan = <1>;
};


/* ADC */
&tscadc {
	status = "okay";
	/* XR, YD, XL, YI, GND, 3.3V/2, 3.3V/2, GND */
	// tsc {
	// 	ti,wires = <4>;
	// 	ti,x-plate-resistance = <200>;
	// 	ti,coordinate-readouts = <5>;
	// 	ti,wire-config = <0x00 0x11 0x22 0x33>;
	// 	ti,charge-delay = <0x400>;
	// };

	adc {
		ti,adc-channels = <5 6>;
	};
};

&mmc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&mmc1_pins_default>;
	status = "okay";
	vmmc-supply = <&vmmc_fixed>;
	bus-width = <4>;
};

&mmc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&wlan_pins_default>;
	status = "okay";
	vmmc-supply = <&vmmc_fixed>;
	bus-width = <1>; // <4>;
	broken-cd; // Tell driver to poll for device insertion
	cap-sd-highspeed; // Try to enable 50Mhz mode
};

&dcan0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&dcan0_pins_default>;
};

&dcan1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&dcan1_pins_default>;
};


&rtc {
	system-power-controller;
};

