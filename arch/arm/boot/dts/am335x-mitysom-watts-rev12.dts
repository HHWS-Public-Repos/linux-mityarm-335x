		        AM33XX_IOPAD(0x8DC, PULL_DISABLE | MUX_MODE7)		/* lcd_data15.lcd_data15 */
		        AM33XX_IOPAD(0x820, PULL_DISABLE | MUX_MODE7)		/* gpmc_ad8.lcd_data16 */
